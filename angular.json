{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@const-developer/ng-lib": {
      "projectType": "library",
      "root": "projects/const-developer/ng-lib",
      "sourceRoot": "projects/const-developer/ng-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/const-developer/ng-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/const-developer/ng-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/const-developer/ng-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/const-developer/ng-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "sandbox": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/sandbox",
      "sourceRoot": "projects/sandbox/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sandbox",
            "index": "projects/sandbox/src/index.html",
            "main": "projects/sandbox/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/sandbox/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/sandbox/src/favicon.ico",
              "projects/sandbox/src/assets"
            ],
            "styles": [
              "projects/sandbox/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sandbox:build:production"
            },
            "development": {
              "browserTarget": "sandbox:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sandbox:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/sandbox/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/sandbox/src/favicon.ico",
              "projects/sandbox/src/assets"
            ],
            "styles": [
              "projects/sandbox/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "const-lib": {
      "projectType": "library",
      "root": "projects/const-lib",
      "sourceRoot": "projects/const-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/const-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/const-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/const-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/const-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "const-lib2": {
      "projectType": "library",
      "root": "projects/const-lib2",
      "sourceRoot": "projects/const-lib2/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/const-lib2/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/const-lib2/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/const-lib2/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/const-lib2/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "@const-developer/ng-const": {
      "projectType": "library",
      "root": "projects/const-developer/ng-const",
      "sourceRoot": "projects/const-developer/ng-const/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/const-developer/ng-const/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/const-developer/ng-const/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/const-developer/ng-const/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/const-developer/ng-const/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
